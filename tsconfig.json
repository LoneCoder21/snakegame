{
    "compilerOptions": {
        "target": "es6",
        "lib": ["dom", "dom.iterable", "es2017"],
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "es2022",
        "isolatedModules": true,
        "noEmit": true
    }
}
